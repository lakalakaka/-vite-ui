{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "pnpm run lint:script && pnpm run lint:style",
    "lint:script": "eslint --ext .js,.jsx,.ts,.tsx,.vue --fix --quiet ./",
    "lint:style": "stylelint --fix \"src/**/*.{css,scss}\"",
    "postinstall": "husky install",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test": "vitest"
  },
  "dependencies": {
    "eslink-ui-plus": "1.0.0-beta.5",
    "vue": "3.2.37"
  },
  "devDependencies": {
    "@amatlash/vite-plugin-stylelint": "^1.2.0",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@iconify-json/ic": "1.1.4",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "2.0.0",
    "@vue/test-utils": "2.0.2",
    "commitlint": "^16.2.4",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "happy-dom": "6.0.4",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-prettier": "^2.0.0",
    "terser": "5.4.0",
    "typescript": "^4.5.4",
    "unocss": "0.45.6",
    "vite": "^3.0.1",
    "vite-plugin-eslint": "^1.6.0",
    "vite-plugin-inspect": "^0.5.1",
    "vite-svg-loader": "^3.3.0",
    "vitepress": "0.22.4",
    "vitepress-theme-demoblock": "1.4.2",
    "vitest": "0.21.1",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.34.7"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts}": [
      "npm run lint:script",
      "git add ."
    ],
    "**/*.{scss}": [
      "npm run lint:style",
      "git add ."
    ]
  }
}